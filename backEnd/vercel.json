{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "outputDirectory": "build",
    "buildCommand": "npm run start",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ]
}
