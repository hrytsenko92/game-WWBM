{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "outputDirectory": "build",
    "routes": [
        {
            "src": "/build/main.js",
            "dest": "/build/main.js"
        }
    ],
    "buildCommand": "npm run start"
}
