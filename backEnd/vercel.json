{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
